{
  "name": "guitar-inventory",
  "version": "1.0.0",
  "description": "Typescript guitar inventory app using Vue, Materialize on the frontend and Node.js with Express, PostgreSQL on the backend",
  "main": "dist/index.js",
  "scripts": {
    "clean": "rimraf dist/*",
    "copy-assets": "ts-node tools/copyAssets",
    "lint": "tslint -c tslint.json -p tsconfig.json --fix",
    "tsc": "tsc",
    "dev:start": "npm-run-all clean lint tsc copy-assets",
    "dev": "nodemon --watch src -e ts,ejs --exec npm run dev:start",
    "start": "node .",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bobmugura/guitar-inventory.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/bobmugura/guitar-inventory/issues"
  },
  "homepage": "https://github.com/bobmugura/guitar-inventory#readme",
  "dependencies": {
    "@okta/oidc-middleware": "^1.0.2",
    "dotenv": "^6.2.0",
    "ejs": "^2.6.1",
    "express": "^4.16.4",
    "express-session": "^1.15.6"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.0",
    "@types/express": "^4.16.0",
    "@types/express-session": "^1.15.11",
    "@types/fs-extra": "^5.0.4",
    "@types/node": "^10.12.18",
    "@types/shelljs": "^0.8.1",
    "fs-extra": "^7.0.1",
    "nodemon": "^1.18.9",
    "npm-run-all": "^4.1.5",
    "rimraf": "^2.6.2",
    "shelljs": "^0.8.3",
    "ts-node": "^7.0.1",
    "tslint": "^5.12.0",
    "typescript": "^3.2.2"
  }
}
